    "utf8": "^3.0.0",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.3",
    "vue-echarts": "^4.1.0",
    "vue-grid-layout": "^2.3.7",
    "vue-query-builder": "github:cotterjd/vue-query-builder",
    "vue-router": "^3.1.5",
    "vuejs-logger": "1.5.4",
    "vuex": "^3.1.2",
    "vuex-pathify": "^1.4.1",
    "vuex-persistedstate": "^2.7.1",
    "xhr": "^2.5.0"
  },

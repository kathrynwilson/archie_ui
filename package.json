{
  "name": "archie_client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service lint --fix && vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "storybook": "start-storybook",
    "build:staging": "vue-cli-service build --mode staging",
    "pull": "git stash && npm run pll && git stash apply",
    "pull2": "git stash && npm run pll2 && git stash apply",
    "pll": "git branch | grep \\* | cut -d ' ' -f2 | git pull origin",
    "pll2": "git pull origin $(git branch | grep \\* | cut -d ' ' -f2)",
    "push": "git branch | grep \\* | cut -d ' ' -f2 | git push origin",
    "push2": "git push origin $(git branch | grep \\* | cut -d ' ' -f2)",
    "start": "npm run serve",
    "start:local": "vue-cli-service serve --mode local",
    "deploy": "ssh root@10.13.37.30 ./deploy_client.sh"
  },
  "dependencies": {
    "@quasar/extras": "^1.5.2",
    "@syncfusion/ej2-vue-querybuilder": "^17.4.49",
    "apollo-boost": "^0.4.7",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "base-64": "^0.1.0",
    "convert-array-to-csv": "1.0.10",
